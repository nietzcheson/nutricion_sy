{% extends '::base.html.twig' %}

{% block contenido %}
{{ parent() }}

<table class="table">
	<thead>
		<tr>
			<th>#</th>
			<th>Empresa</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		{% for empresa in empresas %}
		<tr>
			<td>
				{{ empresa.id }}
			</td>
			<td>
				{{ empresa.empresa }}
			</td>
			<td class="text-right">
				<div class="btn-group">
				  <a type="button" class="btn btn-default" href="{{ url('nutricion_perfil_empresa', {'id': empresa.id }) }}">
						<span class="glyphicon glyphicon-eye-open"></span>
					</a>
				  <button type="button" class="btn btn-default">
						<span class="glyphicon glyphicon-trash"></span>
					</button>
				</div>
			</td>
		</tr>
		{% endfor %}
	</tbody>
</table>





{% endblock %}
